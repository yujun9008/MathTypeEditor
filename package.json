{
  "name": "react-mathtype-editor",
  "version": "1.0.2",
  "description": "A standalone MathType formula editor component based on React, Ant Design, MathLive, and KaTeX",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.types.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**/*",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "vite build && npm run build:types",
    "build:types": "tsc -p tsconfig.types.json",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "math",
    "mathml",
    "latex",
    "mathlive",
    "katex",
    "formula",
    "editor",
    "react",
    "antd",
    "mathematics",
    "equation"
  ],
  "author": "MathType Editor Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yujun9008/MathTypeEditor.git"
  },
  "bugs": {
    "url": "https://github.com/yujun9008/MathTypeEditor/issues"
  },
  "homepage": "https://github.com/yujun9008/MathTypeEditor#readme",
  "peerDependencies": {
    "antd": "^5.0.0",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0"
  },
  "dependencies": {
    "katex": "^0.16.0",
    "mathlive": "^0.98.6"
  },
  "devDependencies": {
    "@types/katex": "^0.16.0",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^3.0.0",
    "less": "^4.1.0",
    "typescript": "^4.9.0",
    "vite": "^4.0.0",
    "vite-plugin-css-injected-by-js": "^4.0.1"
  }
}
