{
  "name": "@math-type/editor",
  "version": "1.0.0",
  "description": "A standalone MathType formula editor component based on React, Ant Design, MathLive, and KaTeX",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs && npm run build:types",
    "build:esm": "tsc -p tsconfig.esm.json --outDir dist/esm",
    "build:cjs": "tsc -p tsconfig.cjs.json --outDir dist/cjs",
    "build:types": "tsc -p tsconfig.types.json",
    "dev": "vite",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "math",
    "mathml",
    "latex",
    "mathlive",
    "katex",
    "formula",
    "editor",
    "react",
    "antd",
    "mathematics",
    "equation"
  ],
  "author": "MathType Editor Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/math-type/editor.git"
  },
  "bugs": {
    "url": "https://github.com/math-type/editor/issues"
  },
  "homepage": "https://github.com/math-type/editor#readme",
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "antd": "^5.0.0"
  },
  "dependencies": {
    "katex": "^0.16.0",
    "mathlive": "^0.90.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/katex": "^0.16.0",
    "typescript": "^4.9.0",
    "vite": "^4.0.0",
    "@vitejs/plugin-react": "^3.0.0",
    "less": "^4.1.0",
    "@types/node": "^18.0.0"
  }
}